 {% if section.settings.quote_icon %}
.testimonials .testimonial-container .testimonial-content blockquote:before {
   content:'';
      position:relative;
      width:100px;
      height:100px;
      background-color:currentcolor;
       mask-image: url({{ section.settings.quote_icon | image_url: width: 100 }});
      -webkit-mask-image: url({{ section.settings.quote_icon | image_url: width: 100 }});
   mask-size: contain; mask-repeat: no-repeat; mask-position: center;
     -webkit-mask-size: contain; -webkit-mask-repeat: no-repeat; -webkit-mask-position: center;
  }
  
   {% endif %}
